{
  "name": "normkeeper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "emulators": "firebase emulators:start --project normkeeper-dev",
    "build": "next build",
    "lint": "next lint",
    "start": "next start",
    "test:unit": "jest --forceExit --maxWorkers=30% \"__tests__/(client|backend)/.*.test.[tj]s\"",
    "watch:test:personal": "jest --watch --maxWorkers=30% \"__tests__/personalTests/.*.test.[tj]s\"",
    "test:e2e": "playwright test",
    "vercel:pull:development:env": "vercel env pull --environment=development .env",
    "vercel:pull:preview:env": "vercel env pull --environment=preview .env",
    "vercel:pull:production:env": "vercel env pull --environment=production .env",
    "test": "jest --maxWorkers=30%",
    "typia:generate:validators": "typia generate --input __tests__/utils/templateModelValidators --output __tests__/utils/modelValidators --project tsconfig.json"
  },
  "dependencies": {
    "@types/react-dom": "^18.2.4",
    "@ungap/structured-clone": "^1.2.0",
    "bootstrap": "^5.3.3",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^10.8.1",
    "firebase-admin": "^12.0.0",
    "idb": "^7.1.1",
    "json5": "^2.2.3",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.1",
    "react-bootstrap-icons": "^1.11.3",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.0",
    "@playwright/test": "^1.29.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.8",
    "@types/ungap__structured-clone": "^1.2.0",
    "@types/uuid": "^9.0.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.1",
    "fake-indexeddb": "^4.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^3.2.4",
    "setimmediate": "^1.0.5",
    "typescript": "^5.1.6",
    "typia": "^5.3.5",
    "uuid": "^9.0.0"
  }
}
