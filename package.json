{
  "name": "normkeeper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "emulators": "firebase emulators:start --project normkeeper-dev",
    "build": "next build",
    "lint": "next lint",
    "start": "next start",
    "local:test:unit": "jest --forceExit --maxWorkers=30% \"__tests__/(client|backend)/.*.test.[tj]s\"",
    "local:watch:test:personal": "jest --watch --maxWorkers=30% \"__tests__/personalTests/.*.test.[tj]s\"",
    "local:test:only:firestore_rules": "jest __tests__/firestore_rules/.*.test.[tj]s",
    "local:test:e2e": "playwright test",
    "server:dev": "cross-env REMOTE_SERVER=inside.env next dev",
    "server:start": "cross-env REMOTE_SERVER=inside.env next start",
    "server:test:unit": "cross-env REMOTE_SERVER=inside.env jest --forceExit --maxWorkers=30% \"__tests__/(client|backend)/.*.test.[tj]s\"",
    "server:watch:test:personal": "cross-env REMOTE_SERVER=inside.env jest --watch --maxWorkers=30% \"__tests__/personalTests/.*.test.[tj]s\"",
    "server:preview:test:e2e": "cross-env REMOTE_SERVER=preview playwright test",
    "server:production:test:e2e": "cross-env REMOTE_SERVER=production playwright test",
    "vercel:pull:development:env": "vercel env pull --environment=development",
    "vercel:pull:preview:env": "vercel env pull --environment=preview",
    "vercel:pull:production:env": "vercel env pull --environment=production",
    "typia:generate:validators": "typia generate --input __tests__/utils/templateModelValidators --output __tests__/utils/modelValidators --project tsconfig.json"
  },
  "dependencies": {
    "@next/font": "^13.4.4",
    "@types/react-dom": "^18.2.4",
    "firebase": "~10.6.0",
    "firebase-admin": "^12.0.0",
    "idb": "^7.1.1",
    "json5": "^2.2.3",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.0",
    "@playwright/test": "^1.29.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.8",
    "@types/uuid": "^9.0.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.0.3",
    "fake-indexeddb": "^4.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^3.2.4",
    "setimmediate": "^1.0.5",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.1",
    "typia": "^5.3.5"
  }
}
